{
    "manifest_version": 3,
    "name": "Golden Access",
    "version": "1.0",
    "description": "CS4352 Project",
    "action": {
      "default_title": "Open Sidebar"
    },
    "icons":{
        "1": "icons/icon.png"
    },
    "side_panel": {
        "default_path": "sidepanel.html"
    },
    "permissions": ["sidePanel", "scripting"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
	"js": ["content.js"],
	"css": ["styles.css"]
      }
    ],
    "background": {
      "service_worker": "service-worker.js"
    }
}
  
